{
  "name": "@syakoo/todo-mock-api",
  "version": "0.0.0",
  "description": "TodoApp Mock API",
  "author": "syakoo",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "lint-fix": "eslint --cache --fix --ignore-path .gitignore . && prettier --cache --write --ignore-path .gitignore .",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build:api-docs": "node ./scripts/generateAPIdocs.js"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syakoo/todo-mock-api.git"
  },
  "bugs": {
    "url": "https://github.com/syakoo/todo-mock-api/issues"
  },
  "homepage": "https://github.com/syakoo/todo-mock-api#readme",
  "dependencies": {
    "js-base64": "^3.7.2",
    "msw": "^0.42.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-import-resolver-typescript": "^3.1.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.7.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint-fix",
    "commit-msg": "node ./scripts/addEmojiCommitPrefix.js"
  }
}
